<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<html>
<head>
    <title>Financial Institution's Summary Data for Allied sectors</title>
</head>
<body>
<style>
    td{ font-size:9px; }
    th{ font-size:10px; }
</style>
<!-- Begin Page Content -->
<div class="container-fluid">
    <input type="hidden" id="report" value="${REPORT}"/>
    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800"></h1>

    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-12">
            <div class="col-md-10">
                <h6 class="m-0 font-weight-bold text-primary align-center">Financial Institution's Summary Data for Allied sectors</h6>
            </div>
        </div>
        <div class="card-body">
            <div class="table-responsive" style="font-size:10px">
                <table class="table table-bordered" id="aspReportTbl" width="100%" cellspacing="0">
                    <thead>
                    <tr><th colspan="2">Reporting Date</th>
                        <th colspan="12">Reporting Date</th></tr>
                    <tr>
                        <th rowspan="2" width="5%">#</th>
                        <th rowspan="2" width="15%">Financial Institution</th>
                        <th colspan="3" width="20%">Trade & Commerce</th>
                        <th colspan="3" width="20%">Housing</th>
                        <th colspan="3" width="20%">Transport</th>
                        <th colspan="3" width="20%">Production & Manufacturing</th>
                    </tr>
                    <tr>
                        <th>Total Loans</th><th>NPL</th><th>Monthly EMI</th>
                        <th>Total Loans</th><th>NPL</th><th>Monthly EMI</th>
                        <th>Total Loans</th><th>NPL</th><th>Monthly EMI</th>
                        <th>Total Loans</th><th>NPL</th><th>Monthly EMI</th>
                    </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- /.container-fluid -->

</body>
</html>
