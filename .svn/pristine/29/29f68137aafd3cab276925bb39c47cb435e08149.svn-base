/* ================== */
img {
    width: 100%;
}

.center-div {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
}

.card label, label {
    font-weight: bold;
}

table th {
    font-weight: bold !important;
    font-size: 12px !important;
    /*background-color: #093b29 !important;*/
    background-color: rgba(49, 132, 252, 0.47) !important;;
}

table td {
    color: #000000 !important;
}

.two {
    background-color: #34ADFF;
    background-image: -webkit-linear-gradient(150deg, #34ADFF 35%, #4CBFFF 35%);
    min-height: 400px;
}

/*.content {*/
/*background-color: #0f6848;*/
/*}*/

#login-box {
    background: #ffffff;
    font-size: 12px;
    font-weight: normal;
    padding: 5px 15px 5px 15px;
    border-radius: 5px;
    box-shadow: 0px 0px 12px #999999 inset;
    margin: 5% auto 0px;
}

.disclaimer {
    font-size: 10px;

}

#login-box .header {
    background-position: 10px 10px;
    background-repeat: no-repeat;
    border-style: solid;
    border-width: 0px 0px 2px 0px;
    border-color: #e3e3e3;
    padding: 40px 15px 10px 0px;
    color: #428BCA;
    font: 20px Arial;
    font-weight: bold;
}

