    float: right;
    line-height: 150%;
}

#accordionLegislation .custom-card,
#accordionPublication .custom-card {
    border-radius: 5px;
    border: 1px solid gray;
}

#accordionLegislation .card-header i.fa-plus-circle,
#accordionLegislation .card-header i.fa-minus-circle,
#accordionPublication .card-header i.fa-plus-circle,
#accordionPublication .card-header i.fa-minus-circle {
    float: right;
    line-height: 150%;
}

