var scriptLoader = function (url) {
    //TODO for production environment
    url = globalConf.context + '/resources/js/' + url + '.js';
    //url = window.location.host+'/resources/js/' + url + '.js';

    $.ajax(
        {
            type: "GET",
            url: url,
            dataType: "script",
            cache: false
            //cache: prodEnv
        }
    );
};
