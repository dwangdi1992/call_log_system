    function _baseURL() {
        return mmsGlobal.baseURL + 'kidu';
    }

    /**
     * To get kidu information if any.
     */
    function getKiduInfoList() {
        var divId = $('#vehicleInfoDiv');
        $.ajax({
            url: _baseURL() + '/getKiduInfoList', type: 'GET', success: function (res) {
                prepareTable($('#kiduTbl'), res);
            }
        })
    }

    /**
     * TO get kidu information by company name.
     */
    function getKiduInfoListByCompany() {
        $('#btnSearch').on('click', function () {
            var company = $('#company').val();
            if (isEmpty(company)) {
                return;
            }
            $.ajax({
                url: _baseURL() + '/getKiduInfoListByCompany',
                type: 'GET',
                data: {company: company},
                success: function (res) {
                    prepareTable($('#kiduTbl'), res);
                }
            })
        });
    }

    /**
     * To prepare the datatable.
     * @param element
     * @param res
     */
    function prepareTable(element, res) {
        var row = '';
        if (!isEmpty(res)) {
            for (var i in res) {
                row = row + '<tr align="left">' + '<td>' + res[i].searchKey + '</td>' + '<td>' + res[i].name + '</td>' + '<td>' + res[i].occupation + '</td>' + '<td>' + res[i].companyName + '</td>' + '<td>' + res[i].sector + '</td>' + '<td>' + commaSeparator(res[i].totalKiduAmt) + '</td>'
            }

            element.dataTable().fnDestroy();
            element.find('tbody').empty().prepend(row);
            element.dataTable();
        } else {
            element.find('tbody').empty()
        }
    }

    function init() {
        getKiduInfoListByCompany();
        // getKiduInfoList();
        $("#company").autocomplete({
            maxShowItems: 20, source: companies
        });
    }

    return {
        init: init
    };
})();

$(document).ready(function () {
    kidu.init();
});
