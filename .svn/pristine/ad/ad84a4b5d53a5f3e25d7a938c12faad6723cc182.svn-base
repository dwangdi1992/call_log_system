<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<html>
<head>
    <title>Deposit Report</title>
</head>
<body>
<!-- Begin Page Content -->
<div class="container-fluid">
    <input type="hidden" id="report" value="${REPORT}"/>
    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Deposit Report</h1>

    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-12">
            <div class="col-md-10">
                <h6 class="m-0 font-weight-bold text-primary align-center">Deposit by type (Amount in millions)</h6>
            </div>
        </div>
        <div class="card-body">
            <div class="table-responsive" style="font-size:10px">
                <table class="table table-bordered" id="depositTableId" width="100%" cellspacing="0">
                    <thead>
                    <tr>
                        <th rowspan="2">#</th>
                        <th rowspan="2">Date</th>
                        <th colspan="2">Savings</th>
                        <th colspan="2">Current</th>
                        <th colspan="2">Recurring</th>
                        <th colspan="2">Fixed</th>
                        <th colspan="2">Total Deposit</th>
                    </tr>
                    <tr>
                        <th>No. A/c</th>
                        <th>Amount (Million)</th>
                        <th>No. A/c</th>
                        <th>Amount (Million)</th>
                        <th>No. A/c</th>
                        <th>Amount (Million)</th>
                        <th>No. A/c</th>
                        <th>Amount (Million)</th>
                        <th>No. A/c</th>
                        <th>Amount (Million)</th>
                    </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<!-- /.container-fluid -->

</body>
</html>
