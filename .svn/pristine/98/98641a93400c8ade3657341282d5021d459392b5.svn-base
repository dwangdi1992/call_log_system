<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>File Uploaded</title>
</head>
<body>
<!-- Begin Page Content -->
<div class="container-fluid">
    <form id="fileUploadForm" method="POST" action="<c:url value='/'/>">
        <!-- Page Heading -->
        <h1 class="h3 mb-2 text-gray-800">Uploaded Files</h1>

        <!-- DataTales Example -->
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">List of Uploaded Files</h6>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered  enableDataTable" id="fileUploadTable" width="100%"
                           cellspacing="0">
                        <thead>
                        <tr>
                            <th width="5%">#</th>
                            <th width="10%">File ID</th>
                            <th width="15%">File Uploaded</th>
                            <th width="45%">File Name</th>
                            <th class="align-center" width="20%">Action</th>
                        </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </form>
</div>
<!-- /.container-fluid -->
</body>
</html>
